    <!-- Fitness Goal Selection -->
    <div class='jumbo programstart'>
      <h1>Fitness Goal</h1>
      <div class='layout-row layout-align-center' style="text-align: center">
      {{#paper-select class="flex-33" label="Select a Goal!" options=fitnessGoals selected=chosenFitnessGoal onChange=(action (mut chosenFitnessGoal)) as |fitness|}}
        {{#if fitness.goal}}
          {{fitness.goal}}
        {{/if}}
      {{/paper-select}}
      </div>
    </div>

    <!-- Muscle Group Selections -->
    {{#if chosenFitnessGoal.goal}}
    <div class='jumbo muscleGroup'>
      <h1>{{retRandMessage}}</h1>
      <h3>Let's get your {{chosenFitnessGoal.goal}} program started.</h3>
      <h4>Choose 2 or 3 muscle groups you'd like to focus on for today!</h4>
      <div class="layout-row layout-align-space-between" style="padding-top: 15px;">
        {{#paper-select class="flex-30" label="Primary Muscle" options=firstGroup selected=chosenMuscleGroup1 onChange=(action "changeGroups" "1") as |muscle|}}
          {{muscle.group}}
        {{/paper-select}}
        {{#paper-select class="flex-30" label="Secondary Muscle" options=secondGroup selected=chosenMuscleGroup2 onChange=(action "changeGroups" "2") as |muscle|}}
          {{muscle.group}}
        {{/paper-select}}
        {{#paper-select class="flex-30" label="(Optional) Assistive Muscle" options=thirdGroup selected=chosenMuscleGroup3 onChange=(action "changeGroups" "3") as |muscle|}}
          {{muscle.group}}
        {{/paper-select}}
      </div>
    </div>
    {{/if}}

    <!-- Muscle Group Selection Confirmation/Selection of Manual or Random Plans -->
    {{#if (gte chosenGroups.length 2)}}
    <div class='jumbo exerciseChoice'>
      <h1>{{retRandMessage2}}</h1>
      <h4>Now choose to either manually select exercises, or generate random exercises for your program.</h4>
      <div class='layout-row layout-align-center' style='padding-top: 15px;'>
        {{#paper-button onClick=(action "clickManual")}}Manual{{/paper-button}}
        {{#paper-button onClick=(action "clickRandom")}}Random{{/paper-button}}
      </div>
    </div>
    {{/if}}

